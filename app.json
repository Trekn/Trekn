{
  "expo": {
    "name": "Trekn",
    "slug": "trekn",
    "version": "0.2.11",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "trekn",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#131313"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "xyz.exploration.trekn",
      "googleServicesFile": "./google/ios.plist",
      "usesAppleSignIn": true,
      "buildNumber": "26",
      "config": {
        "googleSignIn": {
          "reservedClientId": ""
        }
      },
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "instagram",
          "instagram-stories",
          "telegram",
          "tg",
          "messenger",
          "me",
          "m.me"
        ]
      },
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 19,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD2gDXHzVF8I3G-ui_PeKrEcWvWHLq33S4"
        }
      },
      "package": "com.c2tx420.TreknNativeSDK50"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Turning on location to allow us show nearby your location.",
          "locationAlwaysPermission": "Turning on location to allow us show nearby your location.",
          "locationWhenInUsePermission": "Turning on location to allow us show nearby your location."
        }
      ],
      [
        "@react-native-google-signin/google-signin"
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Trekn to access your photos.",
          "savePhotosPermission": "Allow Trekn to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Trekn to access your camera",
          "microphonePermission": "Allow Trekn to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": ""
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": ""
    }
  }
}